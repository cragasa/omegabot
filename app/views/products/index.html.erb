<div class="container">
  <h1>Products</h1>

  <%= paginate @products %>
  
  <div class="row">
      <% @products.each do |product| %>
      <div class="col s4">
      <%= render partial: 'products/product', object: product %>
      </div>
      <% end %>
  </div>
  <%= paginate @products %>
</div>